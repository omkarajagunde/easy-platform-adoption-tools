{
  "name": "client-scripts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "rewire": "^5.0.0",
    "uglify-js": "^3.15.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:prod": "node ./build-rewired.js && npm run clean && npm run move-index",
    "move-prescripts": "uglifyjs public/leader-line.min.js public/plain-draggable.min.js build/temp.js -o build/beeguide-tools.js",
    "clean": "cd build && mv static/js/*.js temp.js && mv static/css/*.css beeguide-tools.css && npm run move-prescripts && rm -r static",
    "move-index": "cp public/index.html build/index.html"
  },
  "devDependencies": {
    "sass": "^1.52.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
